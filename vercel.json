{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "command": "npm install --legacy-peer-deps && npm run build"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "package.json"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  
  
  
  
  
  